<div class="row">
  <div class="col-3">
    <!-- Tab navs -->
    <div
      class="nav flex-column nav-pills text-center"
      id="v-pills-tab"
      role="tablist"
      aria-orientation="vertical"
    >
      <a
        class="nav-link active"
        id="v-pills-link1-tab"
        data-mdb-toggle="pill"
        href="#v-pills-link1"
        role="tab"
        aria-controls="v-pills-link1"
